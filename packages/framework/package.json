{
  "name": "oribella-framework",
  "version": "0.10.2",
  "description": "Normalizes event flows into an understandable consistent api",
  "repository": {
    "type": "git",
    "url": "https://github.com/oribella/framework"
  },
  "main": "dist/commonjs/oribella-framework.js",
  "typings": "dist/commonjs/oribella-framework.d.ts",
  "scripts": {
    "lint": "tslint -c tslint.json --project tsconfig.json",
    "pretest": "npm run lint",
    "test": "aw -c test/aw.config.js",
    "coverage": "aw -c test/aw.config.js | coveralls",
    "clean": "rimraf dist",
    "commonjs": "tsc -p tsconfig.build.json --module commonjs --outDir dist/commonjs",
    "amd": "tsc -p tsconfig.build.json --module amd --outDir dist/amd",
    "amd-bundled": "tsc -p tsconfig.build.json --module amd --outFile dist/oribella-framework.js",
    "system": "tsc -p tsconfig.build.json --module system --outDir dist/system",
    "es2015": "tsc -p tsconfig.build.json --module es2015 --outDir dist/es2015 --target es2015",
    "build": "npm-run-all -n clean --parallel commonjs amd amd-bundled system es2015"
  },
  "author": "Christoffer Åström",
  "license": "MIT",
  "files": [
    "dist",
    "src"
  ],
  "jspm": {
    "registry": "npm",
    "jspmPackage": true,
    "main": "index",
    "format": "amd",
    "directories": {
      "dist": "dist/amd"
    },
    "peerDependencies": {
      "tslib": "^1.5.0"
    },
    "dependencies": {
      "tslib": "^1.5.0"
    }
  },
  "dependencies": {
    "tslib": "^1.9.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/jsdom": "^11.0.4",
    "@types/mocha": "^5.0.0",
    "@types/node": "^9.6.2",
    "@types/sinon": "^4.3.0",
    "@types/sinon-chai": "^2.7.29",
    "after-work.js": "^4.1.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "jsdom": "^11.7.0",
    "mocha": "^5.0.0",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.5.0",
    "sinon-chai": "^3.0.0",
    "ts-node": "^5.0.0",
    "tslint": "^5.9.1",
    "tslint-config-airbnb": "^5.8.0",
    "typescript": "^2.8.1"
  }
}
