{
  "extends": "tslint:latest",
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      false
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type"
    ],
    "interface-name": [
      true,
      "never-prefix"
    ],
    "no-shadowed-variable": false,
    "no-string-literal": false,
    "trailing-comma": [
      false
    ],
    "member-ordering": [
      "fields-first"
    ],
    "no-bitwise": false,
    "no-conditional-assignment": false,
    "max-classes-per-file": [
      0
    ],
    "max-line-length": [
      0
    ],
    "no-empty": false
  }
}
