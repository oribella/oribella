{
  "name": "aurelia-sortable",
  "version": "0.1.0",
  "description": "",
  "license": "MIT",
  "author": "Christoffer Åström",
  "main": "dist/commonjs/index.js",
  "repository": {
    "type": "git",
    "url": "http://github.com/oribella/aurelia-sortable"
  },
  "scripts": {
    "build": "npm run build:commonjs && npm run build:system && npm run build:amd",
    "build:commonjs": "babel src --out-dir dist/commonjs",
    "build:watch:commonjs": "babel src --out-dir dist/commonjs -w",
    "build:system": "babel src --modules system --out-dir dist/system",
    "build:watch:system": "babel src --modules system --out-dir dist/system -w",
    "build:amd": "babel src --modules amd --out-dir dist/amd",
    "build:watch:amd": "babel src --modules amd --out-dir dist/amd -w",
    "test": "mocha ./test --require ./test/global.js --compilers js:babel/register",
    "test:watch": "mocha ./test --require ./test/global.js --compilers js:babel/register -w",
    "lint": "eslint src"
  },
  "devDependencies": {
    "oribella-framework": "git+ssh://github.com/oribella/framework",
    "aurelia-binding": "git+ssh://github.com/aurelia/binding.git",
    "aurelia-dependency-injection": "git+ssh://github.com/aurelia/dependency-injection.git",
    "aurelia-loader": "git+ssh://github.com/aurelia/loader.git",
    "aurelia-logging": "git+ssh://github.com/aurelia/logging.git",
    "aurelia-metadata": "git+ssh://github.com/aurelia/metadata.git",
    "aurelia-path": "git+ssh://github.com/aurelia/path.git",
    "aurelia-task-queue": "git+ssh://github.com/aurelia/task-queue",
    "aurelia-templating": "git+ssh://github.com/aurelia/templating.git",
    "babel": "^5.6.14",
    "babel-eslint": "^3.1.5",
    "chai": "^2.3.0",
    "core-js": "^0.9.18",
    "eslint": "^0.21.0",
    "mocha": "^2.2.4",
    "sinon": "^1.14.1",
    "sinon-chai": "^2.7.0"
  },
  "jspm": {
    "main": "index",
    "format": "amd",
    "directories": {
      "lib": "dist/amd"
    },
    "dependencies": {
      "aurelia-dependency-injection": "github:aurelia/dependency-injection@^0.9.0",
      "aurelia-templating": "github:aurelia/templating@^0.13.2",
      "oribella/default-gestures": "github:oribella/default-gestures@master"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.1.13",
      "babel-runtime": "npm:babel-runtime@^5.1.13",
      "core-js": "npm:core-js@^0.9.4"
    }
  }
}
